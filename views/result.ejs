<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <table>
      <tr>
        <th>Full Name</th>
        <th>Email Address</th>
      </tr>
      <% if(user.length!=0){ let i=1; user.forEach((data) => { %>
      <tr>
        <td><%= i %></td>
        <td><%= data.name %></td>
        <td><%= data.mail %></td>
      </tr>
      <% i++; }) %> <% } else{ %>
      <tr>
        <td>No Data Found</td>
      </tr>
      <% } %>
    </table>
  </body>
</html>
